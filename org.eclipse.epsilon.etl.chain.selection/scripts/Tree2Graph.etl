model Tree driver EMF {
nsuri = "TreeDsl"
};

model Graph driver EMF {
nsuri = "Graph"
};

rule Tree2Node
	transform t:Tree!Tree
	to n : Graph!Node {

	n.label := t.label; //Number of op/exp = 2+2= 4
	var target : Graph!Node := t.parent.equivalent(); //Number of op/exp = 2+2+2= 6
	if (target.isDefined()) { 
		var edge := new Graph!Edge;
		edge.source := target;
		edge.target := n;
	}
	
}